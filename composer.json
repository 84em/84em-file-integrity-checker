{
  "name": "84em/file-integrity-checker",
  "description": "WordPress plugin for scanning and tracking file checksums to detect changes",
  "version": "2.0.0",
  "type": "wordpress-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew Miller",
      "email": "andrew@84em.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "woocommerce/action-scheduler": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "wp-cli/wp-cli": "^2.9",
    "wp-cli/wp-cli-tests": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "EightyFourEM\\FileIntegrityChecker\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EightyFourEM\\FileIntegrityChecker\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test:unit": "phpunit tests/Unit",
    "test:integration": "phpunit tests/Integration"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
